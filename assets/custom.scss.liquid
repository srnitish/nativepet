/* Shop by need slider styles start */
.custom-stars {
  display: flex;
  height: auto;
  width: 18px;
  margin: auto;
  justify-content: center;
  align-items: center;
}
.collection_slider_div {
  padding: 50px 0 0 0!important;
  @media (min-width: 1200px)  {
    margin-left: calc((100% - 1210px) / 2);
  }
  .tabs-thumbs {
    margin: 0;
    padding-left: 0;
    font-size: 18px;
    .tabs-thumb {
      display: inline-block;
      line-height: 3;
      position: relative;
      a {
        display: block;
/*         padding: 0 1.5rem; */
        padding: 0 1.2rem;
        color: #0053a5;
        text-decoration: none;
        cursor: pointer;
        line-height: 2;
        font-weight: normal;
      }
      &.is-active {
        a {
          font-weight: bold;
          &::after, &:hover:after {
            content: "";
            position: absolute;
            bottom: -1px;
            left: 50%;
            height: 4px;
            width: calc(100% - 2.75rem);
            background: #619ed5;
            margin: auto;
            transform: translateX(-50%);
          }
        }
      }
    }
  }
  .tabs-panels {
    margin: 0;
    padding: 0;
    list-style: none;
    .tabs-panel {
      display: none;
    }
  }
  .homepage-product-slider .product-wrap{    
      padding: 10px 0px 0 0px;
      border-radius: 20px;
  }
  .flickity-viewport {
    min-height: 366px;
  }
  h2.title {
    color: #0053a5;
    padding: 0 10px;
    font-family: 'Airo' !important;
    font-size: 40px;
    text-align: left;
  }
  .featured-collection__description.section {
    color: #0053a5;
    font-family: "Airo",serif;
    text-align: left;
    p {
      display: list-item;
      list-style-type: disc;
      list-style-position: inside;
      font-size: 18px;
    }
    h2 {
      color: #0053a5;
      margin-bottom: 20px;
      display: block;
      font-family: "NativePetHand","Regular";
      line-height: 1;
      font-size: 1.6em !important;
      font-weight: 100;
      width: 360px;
    }
  }
  .product-details {
    .preheading-slider{
        line-height: 10px;
        margin-top: 20px;
        font-family: "NativePetHand","Regular";
        font-size: 14px;
    }
    .title {
      color: #0053a5;
      font-size: 20px;
      font-weight: normal;
      font-family: 'Airo' !important;
      margin-top: 15px;
    }
  }
  .products-slider.slider-gallery {
    margin: 0 auto;
    .gallery-cell.visible-3 {
      background: #FFFBF7;
      border-radius: 40px;
      margin: 0 10px;
      &:nth-child(2) {
        left: 32.16%;
      }
      &:nth-child(3) {
        left: 64.32%;
      }
    }
    .gallery-cell {
      display: block;
      min-height: 100%;
      .product_image img {
        max-height: 175px;
      }
      .product-info__caption {
        padding: 0 15px;
      }
    }
  }
  .products-length-1 .flickity-slider {
    transform: none !important;
  }
  .flickity-slider {
    transform: translateX(1.76%);
    left: -15px !important;
  }
  .flickity-page-dots, .flickity-button {
    display: none;
  } 
  .collection-slider {
    width: 100%;
    display: flex;
    flex-wrap: wrap;
    align-items: center;
  } 
  .horizontal-menu {
    display: block;
    color: inherit;
    text-decoration: none;
    padding: 10px 5px;
    .horizontal-menu-items {
      width: 100%;
      white-space: nowrap;
      overflow-x: auto;
      overflow-y: hidden;
      -webkit-appearance: none;
         -moz-appearance: none;
              appearance: none;
      -webkit-overflow-scrolling: touch;
      > .horizontal-menu-item {
        display: inline-block;
        line-height: 1;
        font-family: "Airo",serif;
        padding: 0 5px;
        -webkit-animation-name: slidein-animation;
                animation-name: slidein-animation;
        -webkit-animation-duration: 350ms;
                animation-duration: 350ms;
        -webkit-animation-fill-mode: backwards;
                animation-fill-mode: backwards;
      }
      &::-webkit-scrollbar {
        display: none;
      }
    }
  }
}

/* Media Queries   */
@media (min-width: 481px) {
  .collection_slider_div .products-length-1 .flickity-slider, .collection_slider_div .products-length-2 .flickity-slider, 
  .collection_slider_div .products-length-3 .flickity-slider{
    transform: translateX(0) !important;
    left: 0 !important
  }
}
  
@media (max-width: 991px){
  .collection_slider_div.featured_products.product-slider .columns {
      width: 100%;
  }
}

@media (max-width: 1024px) {
  .tabs-thumb a {
      padding: 0 10px;
  }
  .tabs-thumb.is-active a::after, .tabs-thumb a:hover:after {
      width: calc(100% - 15px);
  }
}
  
@media (max-width: 480px){
  .collection_slider_div .featured-collection__description.section strong {
    display: none;
  }
  .horizontal-menu .horizontal-menu-items {
    width: 100%;
    white-space: nowrap;
    overflow-x: auto;
  }
  .collection_slider_div h2.title {
    padding: 0 18px;
}
  .product-wrap {
      height: 100%;
  }
  .products-slider.slider-gallery .gallery-cell.visible-3 {
      width: calc(70% - 10px);
  }
  .collection_slider_div .collection-slider {
    width: 100vw;
  }
}

/*   Custom Code */
@-webkit-keyframes slidein-animation {
  0% {
    opacity: 0;
  }
  1% {
    transform: translateX(160px);
  }
  100% {
    opacity: 1;
    transform: translateX(0);
  }
}
@keyframes slidein-animation {
  0% {
    opacity: 0;
  }
  1% {
    transform: translateX(160px);
  }
  100% {
    opacity: 1;
    transform: translateX(0);
  }
}
